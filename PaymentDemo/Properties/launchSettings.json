        "ASPNETCORE_CONNECTIONSTRING_PAYMENTDEMO_DB": "Server=.; Database= PaymentsDb; Trusted_Connection=True;",
        "ASPNETCORE_CHEAP_PROVIDER_ENDPOINT": "http://localhost:5001/api", 
        "ASPNETCORE_EXPENSIVE_PROVIDER_ENDPOINT": "http://localhost:5002/api", 
